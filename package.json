{
  "name": "wantable-app",
  "version": "1.0.0",
  "description": "query last.fm data",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "yarn copy && webpack --mode production",
    "copy": "cross-env mkdir -p public && cross-env cp assets/* styles.css index.html node_modules/bulma/css/bulma.min.css public",
    "dev": "yarn copy && webpack && ts-node server.ts"
  },
  "author": "Alex Lurvey",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@vercel/node": "^5.0.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "custom-env": "^2.0.6",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typescript": "^4.2.3",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^0.28.0",
    "bulma": "^0.9.1",
    "express": "^4.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "browserslist": [
    "last 3 and_chr versions",
    "last 3 chrome versions",
    "last 3 opera versions",
    "last 3 ios_saf versions",
    "last 3 safari versions"
  ],
  "packageManager": "yarn@4.5.3",
  "resolutions": {
    "cookie": "0.7.0",
    "path-to-regexp": "6.3.0"
  }
}
